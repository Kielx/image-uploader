{
  "name": "image-uploader",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "firebase": "^9.0.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "react-toggle": "^4.1.2",
    "tailwindcss": "^2.2.8",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run watch:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/tailwind.css -o src/index.css",
    "watch": "chokidar \"./src/**/*.js\" -c \"yarn run watch:css\"",
    "dev": "concurrently \"yarn run watch\" \"yarn run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^10.3.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.2.1",
    "postcss-cli": "^8.3.1"
  }
}
